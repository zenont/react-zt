{
	"name": "react-zt",
	"version": "1.0.0-1",
	"main": "lib/index.js",
	"repository": "https://github.com/zenontrujillo/react-zt.git",
	"license": "MIT",
	"engines": {
		"npm": ">=3",
		"node": ">=6"
	},
	"scripts": {
		"cleanup": "rimraf lib dist doc demo",
		"transpile": "tsc",
		"doc": "./node_modules/.bin/typedoc --out ./doc/ ./src/index.ts",
		"build": "run-s cleanup transpile doc",
		"prepublishOnly": "run-s build",
		"test": "./node_modules/.bin/jest --config jest/config.json",
		"test:coverage": "./node_modules/.bin/jest --config jest/config.json --coverage",
		"test:opener": "./node_modules/.bin/opener coverage/lcov-report/index.html",
		"test:report": "run-s test:coverage test:opener",
		"lint": "tslint -p tsconfig.json --exclude '**/*.d.ts'",
		"webpack": "webpack --config src/demo/webpack.config.ts --env.HOST_PORT=9546 --env.PRODUCTION=true",
		"bundle": "run-s cleanup webpack",
		"serve": "webpack-dev-server --config src/demo/webpack.config.ts --env.HOST_PORT=9546",
		"start": "run-s cleanup \"serve -- --env.COBRAND={1}\" --"
	},
	"dependencies": {
		"@fortawesome/fontawesome": "^1.1.0",
		"classnames": "^2.2.5",
		"react": "^16.2.0",
		"react-dom": "^16.2.0"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.3",
		"@types/enzyme": "^3.1.6",
		"@types/extract-text-webpack-plugin": "^3.0.0",
		"@types/html-webpack-plugin": "^2.30.1",
		"@types/jest": "^22.0.1",
		"@types/optimize-css-assets-webpack-plugin": "^1.3.1",
		"@types/react": "^16.0.34",
		"@types/react-dom": "^16.0.3",
		"@types/react-hot-loader": "^3.0.5",
		"@types/webpack": "^3.8.2",
		"@types/webpack-bundle-analyzer": "^2.9.0",
		"@types/webpack-env": "^1.13.3",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.8",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.0",
		"file-loader": "^1.1.6",
		"html-webpack-plugin": "^2.30.1",
		"jest": "^22.0.4",
		"node-sass": "^4.7.2",
		"npm-run-all": "^4.1.2",
		"opener": "^1.4.3",
		"react-hot-loader": "^3.1.3",
		"resolve-url-loader": "^2.2.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.1",
		"ts-jest": "^22.0.0",
		"ts-loader": "^3.2.0",
		"ts-node": "^4.1.0",
		"tslint": "^5.8.0",
		"tslint-react": "^3.3.3",
		"typedoc": "^0.9.0",
		"typescript": "^2.6.2",
		"webpack": "^3.10.0",
		"webpack-bundle-analyzer": "^2.9.1",
		"webpack-dev-server": "^2.9.7"
	}
}
